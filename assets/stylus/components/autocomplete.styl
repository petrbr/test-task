.autocomplete
	display block
	transition 0.3s
	margin-top 10px
	&:empty
		display none
	&--error
		p
			margin 0
			padding 43px 20px
			text-align center
			font-size 20px
			font-weight 700
			color #b5b5b5
	&__row
		padding 10px 20px
		cursor pointer
		&--selected
			background #efefef
			transition .2s